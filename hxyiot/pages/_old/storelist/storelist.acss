page{
  background-color: #f5f5f5;
}
.section{
  background-color: #ffffff;
  width:750.4rpx;
  margin-top:20rpx;
}
.section .item{
  width:95%;
  height:120rpx;
  line-height: 120rpx;
  margin-left:5%;
  font-size:34rpx;
  color:#333333;
  font-weight:400;
  border-bottom:1rpx solid #ddd;
}
.section .item:last-child{
  border-bottom:none;
}
.section .item view{
  display: inline-block;
}
.section .item image{
  width:54rpx;
  height:54rpx;
  float:right;
  margin:33rpx 30rpx 0 0;
}