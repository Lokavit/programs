}


/* 安全输入键盘 */
.mask{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  position: fixed;
  bottom: 0;
  top:0rpx;
  z-index: 9999;
}
.passward{
  background-color: #fff;
  width:626rpx;
  height:428rpx;
  overflow: hidden;
  position: fixed;
  top:80rpx;
  left:50%;
  margin-left:-313rpx;
  border-radius:20rpx;
}
.passward .title{
  height:90rpx;
  line-height: 90rpx;
  font-size:34rpx;
  font-weight:400;
  color:#333;
  text-indent: 34%;
  border-bottom:1rpx solid #60BCFF;
}
.passward .title image{
  width:30rpx;
  height:30rpx;
  float:right;
  margin:33rpx 30rpx 0 0;
}
.passward view:nth-child(2){
  text-align: center;
  font-size:26rpx;
  padding:18rpx 0 14rpx;
}
.passward view:nth-child(3){
  text-align: center;
  font-size:48rpx;
}
.passward view:nth-child(5){
  color:#108EE9;
  margin-bottom:30rpx;
  /* text-align: right; */
  /* padding-right:30rpx; */
  font-size:28rpx;
}
.passward view:nth-child(5){
  text-align: center;
}
.passward view:nth-child(5) text:nth-child(1){
  display: block;
  color:#B2B2B2;
  margin-top:50rpx;
  font-size:24rpx;
}
.passward view:nth-child(5) text:nth-child(2){
  display: block;
  color:#108EE9;
  float: right;
  padding-right:30rpx;
  font-size:28rpx;
  margin-bottom:30rpx;
}
.p_flex{
  display: flex;
  flex-direction: row;
  width:562rpx;
  height:80rpx;
  line-height: 80rpx;
  border:1rpx solid #ddd;
  margin:28rpx auto;
  text-align: center;
}
.passward .p_flex .p_item{
  width:92rpx;
  height:80rpx;  
  border-right:1rpx solid #ddd;
  font-size:40rpx;
  padding:0;
  text-align: center;
  color:#333;
}
.p_flex .p_item:last-child{
  width:92rpx;
  border-right:none;
}
