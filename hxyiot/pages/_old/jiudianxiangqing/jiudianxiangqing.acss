/* pages/shoukuanxiangqing/shoukuanxiangqing.wxss */
page{
  height:100%;
  background-color: #f5f5f5;
  color: #333333;
}
.right{
  float:right;
  
}
.down {
  content: '';
  display: inline-block;
  width: 20rpx;
  height: 20rpx !important;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
  float:right;
  margin-top:35rpx;
}
.up{
  content: '';
  display: inline-block;
  width: 10px;
  height:10px !important;
  border-top: 1px solid #656565;
  border-right: 1px solid #656565;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  float:right;
  margin-top:35rpx;
  margin-right:10rpx;
}



.section{
  width:750.4rpx;
  background-color: #fff;
  margin-bottom:20rpx;
  /* padding:0 30rpx; */
  overflow: hidden;
}
.shoukuan{
  text-align: center;
